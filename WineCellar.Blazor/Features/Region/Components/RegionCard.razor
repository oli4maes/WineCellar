<MudLink OnClick="NavigateToRegionDetail" Underline="Underline.None">
    <MudCard Elevation="2">
        <MudCardHeader>
            <CardHeaderContent>
                <MudText Typo="Typo.h5" Class="mb-2" Color="Color.Primary">@Region?.Name</MudText>
                <MudText Typo="Typo.h6">@Region?.CountryName</MudText>
            </CardHeaderContent>
            <CardHeaderActions>
            </CardHeaderActions>
        </MudCardHeader>
        <MudCardContent>
        </MudCardContent>
        <MudCardActions></MudCardActions>
    </MudCard>
</MudLink>